<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:ProjectHydraDesktop.TacticalEditor">
    <DataTemplate x:Key="None">

    </DataTemplate>
    <DataTemplate x:Key="Infantry">
        <Grid  HorizontalAlignment="Center" VerticalAlignment="Center">
            <Line X1="0" X2="50" Y2="30" Stroke="Black" />
            <Line X1="0" X2="50" Y1="30" Y2="0" Stroke="Black" />
        </Grid>
    </DataTemplate>
    <DataTemplate x:Key="MechanizedInfantry">
        <Grid  HorizontalAlignment="Center" VerticalAlignment="Center">
            <Line X1="0" X2="50" Y2="30" Stroke="Black" />
            <Line X1="0" X2="50" Y1="30" Y2="0" Stroke="Black" />
            <Ellipse Width="40" Height="20" Stroke="Black" />
        </Grid>
    </DataTemplate>
    <DataTemplate x:Key="MotorizedInfantry">
        <Grid HorizontalAlignment="Center" VerticalAlignment="Center">
            <Line X1="0" X2="50" Y2="30" Stroke="Black" />
            <Line X1="0" X2="50" Y1="30" Y2="0" Stroke="Black" />
            <Line X1="25" X2="25" Y1="30" Y2="0" Stroke="Black" />
        </Grid>
    </DataTemplate>
    <DataTemplate x:Key="Armour">
        <Grid HorizontalAlignment="Center" VerticalAlignment="Center">
            <Ellipse Width="40" Height="20" Stroke="Black" />
        </Grid>
    </DataTemplate>
</ResourceDictionary>